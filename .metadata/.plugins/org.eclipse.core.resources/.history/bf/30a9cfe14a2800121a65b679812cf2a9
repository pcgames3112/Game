public class GGDrawable {
	
	public static GGDrawable create() {
		GGDrawable newDrawable = new GGDrawable();
		
		if(newDrawable.init()) {
			return newDrawable;
		}
		return null;	
	}
	
	private GGDrawable() {
		// TODO Auto-generated constructor stub
	}
	
	public void draw(){}
	
	
}
